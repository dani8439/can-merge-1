	"devDependencies": {
		"@ljharb/eslint-config": "^20.2.3",
		"aud": "^2.0.0",
		"eslint": "=8.8.0",
		"in-publish": "^2.0.1",
		"markdown-magic": "^2.6.0",
		"nyc": "^15.1.0",
		"object.fromentries": "^2.0.5",
